            "schema": {
              "type": "number",
              "format": "double",
              "example": 42.02531667
            }
          },
          {
            "name": "lon",
            "in": "query",
            "required": true,
            "description": "longitudine",
            "schema": {
              "type": "number",
              "format": "double",
              "example": 13.45065
            }
          },
          {
            "name": "radius",
            "in": "query",
            "required": true,
            "description": "raggio in km",
            "schema": {
              "type": "integer",
              "example": 50
